{
    "name": "radutopala/google-map-bundle",
    "type": "symfony-bundle",
    "description": "Provides a google map integration for your Symfony2 Project, together with InfoBox object.",
    "keywords": ["google map", "infobox"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Radu Topala",
            "email": "radu.topala@trisoft.ro"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": "2.*",
        "radutopala/google-map": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "symfony/yaml": "2.*",
        "twig/twig": "1.*",
        "kriswallsmith/buzz": "*",
        "willdurand/geocoder": "*"
    },
    "autoload": {
        "psr-0": { "Ivory\\GoogleMapBundle": "" }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "target-dir": "Ivory/GoogleMapBundle"
}
