            id="ivory_google_map.helper.marker_image"
            class="%ivory_google_map.marker_image.helper%"
            public="false"
        >
            <argument type="service" id="ivory_google_map.helper.point" />
            <argument type="service" id="ivory_google_map.helper.size" />
        </service>

