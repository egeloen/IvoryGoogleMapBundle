        <parameter key="ivory_google_map.marker_image.helper">Ivory\GoogleMap\Templating\Helper\Overlays\MarkerImageHelper</parameter>
        <parameter key="ivory_google_map.marker_shape.helper">Ivory\GoogleMap\Templating\Helper\Overlays\MarkerShapeHelper</parameter>
        <parameter key="ivory_google_map.animation.helper">Ivory\GoogleMap\Templating\Helper\Overlays\AnimationHelper</parameter>

        <parameter key="ivory_google_map.info_window.helper">Ivory\GoogleMap\Templating\Helper\Overlays\InfoWindowHelper</parameter>
        <parameter key="ivory_google_map.polyline.helper">Ivory\GoogleMap\Templating\Helper\Overlays\PolylineHelper</parameter>
        <parameter key="ivory_google_map.polygon.helper">Ivory\GoogleMap\Templating\Helper\Overlays\PolygonHelper</parameter>
        <parameter key="ivory_google_map.rectangle.helper">Ivory\GoogleMap\Templating\Helper\Overlays\RectangleHelper</parameter>
        <parameter key="ivory_google_map.circle.helper">Ivory\GoogleMap\Templating\Helper\Overlays\CircleHelper</parameter>
        <parameter key="ivory_google_map.ground_overlay.helper">Ivory\GoogleMap\Templating\Helper\Overlays\GroundOverlayHelper</parameter>

        <parameter key="ivory_google_map.kml_layer.helper">Ivory\GoogleMap\Templating\Helper\Layers\KMLLayerHelper</parameter>

        <parameter key="ivory_google_map.event_manager.helper">Ivory\GoogleMap\Templating\Helper\Events\EventManagerHelper</parameter>
        <parameter key="ivory_google_map.event.helper">Ivory\GoogleMap\Templating\Helper\Events\EventHelper</parameter>

